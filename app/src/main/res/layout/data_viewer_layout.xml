<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:id="@+id/mChannel1TextView"
        android:textColor="#000000"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="99dp" />
    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel2TextView"
        android:layout_alignTop="@+id/mChannel1TextView"
        android:layout_toRightOf="@+id/mChannel1TextView"
        android:layout_toEndOf="@+id/mChannel1TextView"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel3TextView"
        android:layout_alignTop="@+id/mChannel2TextView"
        android:layout_toRightOf="@+id/mChannel2TextView"
        android:layout_toEndOf="@+id/mChannel2TextView"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp" />
    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel4TextView"
        android:layout_alignTop="@+id/mChannel3TextView"
        android:layout_toRightOf="@+id/mChannel3TextView"
        android:layout_toEndOf="@+id/mChannel3TextView"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 1"
        android:textColor="#000fff"
        android:id="@+id/textView1"
        android:layout_above="@+id/mChannel2TextView"
        android:layout_alignLeft="@+id/mChannel1TextView"
        android:layout_alignStart="@+id/mChannel1TextView" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 2"
        android:textColor="#000fff"
        android:id="@+id/textView2"
        android:layout_above="@+id/mChannel3TextView"
        android:layout_alignLeft="@+id/mChannel2TextView"
        android:layout_alignStart="@+id/mChannel2TextView" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 3"
        android:textColor="#000fff"
        android:id="@+id/textView3"
        android:layout_above="@+id/mChannel4TextView"
        android:layout_alignLeft="@+id/mChannel3TextView"
        android:layout_alignStart="@+id/mChannel3TextView" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 4"
        android:textColor="#000fff"
        android:id="@+id/textView4"
        android:layout_above="@+id/mChannel4TextView"
        android:layout_alignLeft="@+id/mChannel4TextView"
        android:layout_alignStart="@+id/mChannel4TextView" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:id="@+id/mChannel5TextView"
        android:textColor="#000000"
        android:layout_marginTop="144dp"
        android:layout_below="@+id/mChannel1TextView"
        android:layout_alignLeft="@+id/mChannel1TextView"
        android:layout_alignStart="@+id/mChannel1TextView" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel6TextView"
        android:layout_alignTop="@+id/mChannel5TextView"
        android:layout_alignLeft="@+id/mChannel2TextView"
        android:layout_alignStart="@+id/mChannel2TextView" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel7TextView"
        android:layout_alignTop="@+id/mChannel6TextView"
        android:layout_alignLeft="@+id/mChannel3TextView"
        android:layout_alignStart="@+id/mChannel3TextView" />

    <TextView
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:text="0.0"
        android:textSize="40dp"
        android:textColor="#000000"
        android:id="@+id/mChannel8TextView"
        android:layout_alignTop="@+id/mChannel7TextView"
        android:layout_alignLeft="@+id/mChannel4TextView"
        android:layout_alignStart="@+id/mChannel4TextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 5"
        android:textColor="#000fff"
        android:id="@+id/textView5"
        android:layout_above="@+id/mChannel6TextView"
        android:layout_alignLeft="@+id/mChannel5TextView"
        android:layout_alignStart="@+id/mChannel5TextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 6"
        android:textColor="#000fff"
        android:id="@+id/textView6"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignLeft="@+id/mChannel6TextView"
        android:layout_alignStart="@+id/mChannel6TextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 7"
        android:textColor="#000fff"
        android:id="@+id/textView7"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/mChannel7TextView"
        android:layout_alignStart="@+id/mChannel7TextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ch. 8"
        android:textColor="#000fff"
        android:id="@+id/textView8"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignLeft="@+id/mChannel8TextView"
        android:layout_alignStart="@+id/mChannel8TextView" />

    <Button
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="GRAPH"
        android:id="@+id/btnGraph"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="66dp" />

</RelativeLayout>